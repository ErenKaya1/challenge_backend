<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewData["Title"] - Challenge Backoffice</title>

    <link href="https://fonts.googleapis.com/css?family=Roboto:400,300,100,500,700,900" rel="stylesheet"
        type="text/css">
    <link href="/css/icons/icomoon/styles.min.css" rel="stylesheet" type="text/css">

    <link href="/lib/bootstrap/bootstrap.min.css" rel="stylesheet" type="text/css">
    <link href="/lib/bootstrap/bootstrap-limitless.min.css" rel="stylesheet" type="text/css">
    <link href="/css/layout.min.css" rel="stylesheet" type="text/css">
    <link href="/css/components.min.css" rel="stylesheet" type="text/css">
    <link href="/css/colors.min.css" rel="stylesheet" type="text/css">
    <link href="/lib/sweetalert/sweetalert2.min.css" rel="stylesheet" type="text/css">
    <link href="/css/custom.css" rel="stylesheet" type="text/css">

    <script src="/lib/jquery/jquery.min.js"></script>
    <script src="/lib/jquery-cookie/jquery-cookie.js"></script>
    <script src="/lib/bootstrap/popper.min.js"></script>
    <script src="/lib/bootstrap/bootstrap.min.js"></script>
    <script src="/lib/sweetalert/sweetalert2.all.min.js"></script>
    <script src="/lib/dropzone/dropzone.js"></script>
    <script src="/lib/loading-overlay/loading-overlay.js"></script>
    <script src="/lib/moment/moment.js"></script>

    <script src="/js/app.js"></script>
    @RenderSection("Heads", false)
</head>

<body>
    @await Component.InvokeAsync("HeaderMenu")
    @RenderBody()
    @await Component.InvokeAsync("Footer")

    <script>
        $(".show-filter-area").on("click", function () {
            $(".filter-card").fadeToggle(150);
            $(".collapse-filter-area").trigger("click");
        });
    </script>
    @RenderSection("Scripts", false)
</body>

</html>